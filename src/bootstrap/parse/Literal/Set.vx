import p from '@/parse/parse.vx';

import @/parse/CommaSequence.vx;
import @/util/SetFromArray.vx;

import ./DeferLiteral.vx;

RawSet := p.SequenceOne([
  [p.Exact('#[')],
  CommaSequence(DeferLiteral),
  [p.Exact(']')],
]);

Set := p.Transform(
  RawSet,
  func(value) => ['literal', SetFromArray(value)],
);

return Set;
