import ./TopExpression.vx;

return func(statement) {
  [type, data, pos] := statement;
  notes := [];
  naming := [];

  if (type == 'note') {
    notes ++= [data];
  } else if (type == 'expression') {
    valData := TopExpression(data);
    naming ++= valData.naming;
    notes ++= valData.notes;
  }

  return [{type, data, naming, pos}, notes];
};
