import ./Statement.vx;

return func([type, statements]) {
  assert type == 'block'; // TODO: Destructure exact matches using literals

  vStatements := [];
  notes := [];

  for (statement of statements) {
    [vs, ns] := Statement(statement);
    vStatements ++= [vs];
    notes ++= ns;
  }

  return [['block', vStatements], notes];
};
