import ./TopExpression.vx;

return func(statement) {
  [type, value, pos] := statement;
  notes := [];

  if (type == 'invalid') {
    return [statement, [statement]];
  }
  
  if (type == 'expression') {
    [value, notes] = TopExpression(value);
    return [[type, value, pos], notes];
  }

  assert false;
  return null;
};
