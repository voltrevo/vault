import ./SubExpression.vx;
import ./TopExpression.vx;

return func(statement) {
  [type, data, _] := statement;

  if (type in #['return', 'assert', 'log.info', 'log.warn', 'log.error']) {
    return SubExpression(data);
  }

  return switch (type) {
    'note' => [data];
    'expression' => TopExpression(data);
  };
};
