import p from './parse.vx';

import ./OW.vx;

Comma := p.Exact(',');

return func(Item) {
  RawList := p.Sequence([
    p.Many(p.SequenceOne([
      [OW],
      Item,
      [OW],
      [Comma],
    ])),
    p.Optional(p.SequenceOne([[OW], Item])),
    [OW],
  ]);

  return p.Transform(RawList, func([vals, lastVals]) => vals ++ lastVals);
};
