import p from '@/util/parse.vx';

import @/parser/CommaSequence.vx;
import @/parser/Invalid.vx;

import ./DeferExpression.vx;

RawSet := p.SequenceOne([
  [p.Exact('#[')],
  CommaSequence(DeferExpression),
  [p.Exact(']')],
]);

Set := p.Transform(
  RawSet,
  func(values) {
    invalid := Invalid(values);

    if (invalid:Kind() != 'null') {
      return invalid;
    }

    return ['set', values];
  },
);

return Set;
